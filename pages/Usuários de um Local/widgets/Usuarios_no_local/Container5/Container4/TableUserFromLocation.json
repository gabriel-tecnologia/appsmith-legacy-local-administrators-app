  "tableData": "{{function(){\nSelectUsersFromLocationProd.data.forEach((row)=>{\n\tif (row.Cargos.includes(\"ADMIN\")) {\n\t\trow[\"Acesso de Câmeras\"] = \"ALL_CAMERAS\"\n\t\trow[\"Acesso de Streams\"] = \"LIVE_AND_HISTORY\"\n\t}\n\tlet userInfo = GetUserInformationFromIdsProd.data.find(u => u.legacyuserId == row.userId);\n\trow[\"Nome\"] = userInfo?.Nome;\n\trow[\"CPF\"] = userInfo?.CPF;\n\trow[\"Email\"] = userInfo?.Email;\n\trow[\"Celular\"] = userInfo?.Celular;\n});\nreturn SelectUsersFromLocationProd.data\n}()\n}}",
